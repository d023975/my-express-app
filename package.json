{
	"name": "my-express-app",
	"version": "1.0.0",
	"description": "gen-expreass",
	"nodemonConfig": {
		"exec": "npm run dev",
    "watch": [ "./my-express-app"  ],
		"ignore": [ "**/__tests__/**", "*.test.js", "*.spec.js" ]
	},
	"scripts": {
		"start": "npm run prod",
		"start2": "node ./my-express-app/bin/www",
		"start3": "node ./dist/bin/www",
		"transpile": "babel ./my-express-app --out-dir ./dist --ignore ./node_modules/**/*.js --copy-files --no-copy-ignored",
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "rimraf dist",
		"build": "npm-run-all clean transpile",
		"dev": "SET NODE_ENV=development & SET PORT=4446  & npm-run-all build start3",
		"prod": "SET NODE_ENV=production  & npm-run-all build start3",
		"watch:dev": "nodemon -e js,pug"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/d023975/my-express-app.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/d023975/my-express-app/issues"
	},
	"homepage": "https://github.com/d023975/my-express-app#readme",
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"nodemon": "^2.0.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2"
	},
	"babel": {
		"presets": [ "@babel/preset-env" ]
	}
}
